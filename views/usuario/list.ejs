<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usu√°rio - LIST</title>
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Senha</th>
            <th>Foto</th>
        </tr>

        <% Usuarios.forEach(function(usuario){ %> 
            <tr>
                <td> <%=usuario._id%> </td>
                <td> <%=usuario.nome%> </td>
                <td> <%=usuario.email%> </td>
                <td> <%=usuario.senha%> </td>
                <td> <%=usuario.foto%> </td>
            </tr>
        <% }) %>

    </table>
    
</body>
</html>